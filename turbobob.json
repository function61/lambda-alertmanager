{
	"for_description_of_this_file_see": "https://github.com/function61/turbobob",
	"version_major": 1,
	"project_name": "lambda-alertmanager",
	"builders": [
		{
			"name": "alertmanager",
			"uses": "docker://fn61/buildkit-golang:20190514_1436_703044aa",
			"mount_source": "",
			"mount_destination": "/go/src/github.com/function61/lambda-alertmanager",
			"workdir": "/go/src/github.com/function61/lambda-alertmanager",
			"commands": {
				"build": ["bin/build.sh"],
				"dev": ["bash"]
			}
		},
		{
			"name": "alertmanager-canary",
			"uses": "docker://fn61/buildkit-js:20190412_1139_7d7cd1b0",
			"mount_destination": "/workspace",
			"workdir": "/workspace/alertmanager-canary",
			"commands": {
				"build": ["bin/build.sh"],
				"dev": ["bash"]
			}
		},
		{
			"name": "publisher",
			"uses": "docker://fn61/buildkit-publisher:20190206_1058_d35b3939",
			"mount_destination": "/workspace",
			"commands": {
				"publish": ["publish.sh", "rel/*"],
				"dev": ["bash"]
			},
			"env": {
				"BINTRAY_PROJECT": "function61/dl/lambda-alertmanager"
			},
			"pass_envs": [
				"BINTRAY_CREDS"
			]
		}
	],
	"os_arches": {
		"linux-amd64": true
	}
}
